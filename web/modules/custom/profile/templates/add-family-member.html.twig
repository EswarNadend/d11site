<div class="flex justify-center">
	<div class="container px-14 my-account-wrapper">
		<div
			class="flex justify-between items-center">
			<!-- Back Arrow Button -->
			<img
			src="themes/custom/engage_theme/images/Profile/reverse-arrow.svg" alt="Back Arrow" class="w-5 sm:w-7 cursor-pointer object-contain" onclick="history.go(-1)"/>

			<!-- Heading -->
			<h1 class="text-xl sm:text-2xl md:text-5xl font-bold text-center mt-8 md:mt-12 mb-8 md:mb-20 translateLabel search_field" label-alias="la_my_profile">
				Add Family Member
			</h1>
			<!-- Placeholder for symmetry -->
			<div class="w-5 sm:w-7"></div>
		</div>

		<div class="flex justify-center items-center form-sec border-inherit border-2 rounded-xl p-4 lg:px-10 lg:py-12 bg-white text-center lg:text-start s:mb-24 xs:mb-20">
			<div id="engage_form_input" clas="p-4 items-center inline-block mt-[65px] mb-[65px]">
				<div class="gridding grid grid-cols-2 gap-x-[100px] justify-center items-center w-full max-w-full box-border">
					<div class="contents">
						{{ form.first_name }}
						{{ form.calendar }}
					</div>
					<div class="contents">
						{{ form.gender }}
						{{ form.relations }}
					</div>
					<div class="contents">
						{{ form.phone_number }}
						{{ form.email }}
					</div>
				</div>
				<div class="nonGridding">
					{{ form.upload_file }}
					{{ form.terms }}
					<div class="flex mt-10 gap-4">
						{{ form.actions.submit }}
						{{ form.actions.cancel }}
					</div>
				</div>
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}

			</div>
		</div>
	</div>
